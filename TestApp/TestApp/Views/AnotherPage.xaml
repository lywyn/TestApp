<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TestApp.Views.AnotherPage"
             NavigationPage.HasNavigationBar="False">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Welcome to Another Page"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
            <Button Text="NavigationPage/MainPage" Command="{Binding GoMainPage}" />
            <Button Text="GoBackAsync" Command="{Binding GoBack}" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>